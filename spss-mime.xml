<?xml version="1.0"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <mime-type type="application/x-spss-sav">
    <comment>SPSS Data File Format</comment>
    <magic priority="50">
      <match type="string" offset="0" value="$FL" />
    </magic>
    <glob pattern="*.sav"/>
  </mime-type>
</mime-info>
